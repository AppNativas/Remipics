<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashConAlbum">


    <View
        android:id="@+id/frame_nav"
        android:layout_width="392dp"
        android:layout_height="71dp"
        android:background="@color/azul_claro"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/logo_img_2"
        android:layout_width="232dp"
        android:layout_height="53dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.089"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01"
        app:srcCompat="@drawable/logo_img_2" />

    <ImageView
        android:id="@+id/icon_filtro_f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.759"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.035"
        app:srcCompat="@drawable/icon_filtro_f" />

    <ImageView
        android:id="@+id/icon_cuenta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.909"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.035"
        app:srcCompat="@drawable/icon_cuenta" />

    <TextView
        android:id="@+id/titulo_album"
        android:layout_width="183dp"
        android:layout_height="39dp"
        android:text="Mis Ã¡lbumes"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.132" />



    <ImageView
        android:id="@+id/icon_crear_album"
        android:layout_width="90dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.939"
        app:srcCompat="@drawable/icon_crear_album" />

    <View
        android:id="@+id/frame_albumes"
        android:layout_width="344dp"
        android:layout_height="185dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.279" />

    <View
        android:id="@+id/frame_album"
        android:layout_width="146dp"
        android:layout_height="185dp"
        app:layout_constraintBottom_toBottomOf="@+id/frame_albumes"
        app:layout_constraintEnd_toEndOf="@+id/frame_albumes"
        app:layout_constraintHorizontal_bias="0.156"
        app:layout_constraintStart_toStartOf="@+id/frame_albumes"
        app:layout_constraintTop_toTopOf="@+id/frame_albumes"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/img_portada_album"
        android:layout_width="146dp"
        android:layout_height="128dp"
        android:background="@color/btn_secundario"
        app:layout_constraintBottom_toBottomOf="@+id/frame_album"
        app:layout_constraintEnd_toEndOf="@+id/frame_album"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/frame_album"
        app:layout_constraintTop_toTopOf="@+id/frame_album"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/nom_album"
        android:layout_width="101dp"
        android:layout_height="22dp"
        android:layout_marginTop="24dp"
        android:text="mi 5 cumple"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/frame_album"
        app:layout_constraintEnd_toEndOf="@+id/frame_album"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="@+id/frame_album"
        app:layout_constraintTop_toTopOf="@+id/frame_album"
        app:layout_constraintVertical_bias="0.784" />

    <TextView
        android:id="@+id/cant_memoria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="0"
        android:textSize="13dp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/frame_album"
        app:layout_constraintEnd_toEndOf="@+id/frame_album"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="@+id/frame_album"
        app:layout_constraintTop_toTopOf="@+id/frame_album"
        app:layout_constraintVertical_bias="0.909" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/frame_album"
        app:layout_constraintEnd_toEndOf="@+id/frame_album"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/frame_album"
        app:layout_constraintTop_toTopOf="@+id/frame_album"
        app:layout_constraintVertical_bias="0.881"
        app:srcCompat="@drawable/icon_puntos" />


</androidx.constraintlayout.widget.ConstraintLayout>